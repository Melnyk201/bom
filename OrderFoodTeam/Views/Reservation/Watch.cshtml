@model IEnumerable<OrderFoodTeam.Models.Reservation>
@{
    ViewBag.Title = "AllReservs";
}



<p>AllReservs:</p>
<table class="table">
    <tr>
        <td>Delete</td>
        <td>Name</td>
        <td> Quantity</td>
        <td>ReservationDate</td>
        <td>ReservationTime</td>


    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>



                <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-primary" asp-fragment="delete">Delete</a>



            </td>
            <td> @item.FullName</td>
            <td>@item.Quantity</td>
            <td>@item.ReservationDate </td>
            <td>@item.ReservationTime</td>


        </tr>
    }
</table>